{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "warpc",
      "camelcase": "WArpc",
      "title": "warpc",
      "scope": "source.warpc",
      "file-types": [
        ".wa.rpc"
      ],
      "injection-regex": "^warpc$",
      "class-name": "TreeSitterWarpc"
    }
  ],
  "metadata": {
    "version": "1.0.0",
    "license": "MIT",
    "description": "Warpc grammar for tree-sitter",
    "authors": [
      {
        "name": "アクゼスティア",
        "email": "akzestia@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-warpc"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}